<h1>Listing soundscans</h1>

<table>
  <tr>
    <th>Year</th>
    <th>Sales</th>
    <th>Album</th>
  </tr>

<% @soundscans.each do |soundscan| %>
  <tr>
    <td><%=h soundscan.year %></td>
    <td><%=h soundscan.sales %></td>
    <td><%=h soundscan.album_id %></td>
    <td><%= link_to 'Show', soundscan %></td>
    <td><%= link_to 'Edit', edit_soundscan_path(soundscan) %></td>
    <td><%= link_to 'Destroy', soundscan, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New soundscan', new_soundscan_path %>